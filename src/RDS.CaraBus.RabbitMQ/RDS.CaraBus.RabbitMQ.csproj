<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard1.6;net462</TargetFrameworks>
    <Description>Lightweight service bus for .NET</Description>
    <VersionPrefix>0.0.1</VersionPrefix>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <Product>RDS.CaraBus.RabbitMQ</Product>
    <AssemblyName>RDS.CaraBus.RabbitMQ</AssemblyName>
    <PackageId>RDS.CaraBus.RabbitMQ</PackageId>
    <PackageTags>servicebus;bus;rabbitmq;lightweight;easy;simple</PackageTags>
    <PackageProjectUrl>https://github.com/Toxu4/RDS.CaraBus</PackageProjectUrl>
    <PackageLicenseUrl>http://www.opensource.org/licenses/mit-license.php</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/Toxu4/RDS.CaraBus</RepositoryUrl>
    <Authors>Anton Krasilshchikov</Authors>
    <NoWarn>1701;1702;1705;1591</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.2" />
    <PackageReference Include="RabbitMQ.Client" Version="4.1.3" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\RDS.CaraBus\RDS.CaraBus.csproj">
      <ReferenceOutputAssembly></ReferenceOutputAssembly>
    </ProjectReference>
  </ItemGroup>
</Project>